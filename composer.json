{
    "name": "zain/laravel-doctrine-jetpack",
    "description": "Helpful generators for Doctrine in Laravel",
    "type": "library",
	"keywords": [
		"doctrine",
		"laravel",
		"fluent",
		"generator",
		"mapping",
		"entity"
	],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^4.0"
    },
	"suggest": {
		"laravel-doctrine/orm": "^1.0",
		"laravel-doctrine/fluent": "^1.0",
		"ramsey/uuid-doctrine": "^1.5"
	},
	"autoload": {
		"psr-4": {
			"Zain\\LaravelDoctrine\\Jetpack\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Zain\\LaravelDoctrine\\JetpackServiceProvider"
			]
		}
	},
	"scripts": {
		"test": "vendor/bin/phpunit --colors"
	},
    "license": "MIT",
    "authors": [
        {
            "name": "Zain Mehdi",
            "email": "szainmehdi@gmail.com"
        }
    ]
}
